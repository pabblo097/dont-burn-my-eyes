{
   "name": "dont-burn-my-eyes",
   "version": "0.0.1",
   "description": "Simple screen dimmer",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/pabblo097/dont-burn-my-eyes.git"
   },
   "scripts": {
      "build": "tsc --noEmit && vite build",
      "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
      "build:watch": "cross-env __DEV__=true vite build -w --mode development",
      "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
      "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
      "wss": "node utils/reload/initReloadServer.js",
      "dev": "pnpm build:hmr && (run-p wss build:watch)",
      "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
      "test": "vitest",
      "commitlint": "commitlint --edit",
      "lint": "eslint src --ext .ts,.js,.tsx,.jsx",
      "lint:fix": "pnpm lint --fix",
      "prettier": "prettier . --write",
      "prettier-eslint": "prettier-eslint \"**/*.{js,ts,jsx,tsx}\" --write --config \"./.prettierrc\"",
      "prepare": "husky install"
   },
   "type": "module",
   "dependencies": {
      "@chakra-ui/icons": "^2.1.1",
      "@chakra-ui/react": "2.5.5",
      "@emotion/cache": "^11.11.0",
      "@emotion/react": "^11.11.4",
      "@emotion/styled": "^11.11.0",
      "construct-style-sheets-polyfill": "3.1.0",
      "framer-motion": "^11.0.8",
      "lodash-es": "^4.17.21",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-icons": "^5.0.1",
      "webextension-polyfill": "0.10.0"
   },
   "devDependencies": {
      "@commitlint/cli": "18.4.4",
      "@commitlint/config-conventional": "18.1.0",
      "@rollup/plugin-sucrase": "^5.0.2",
      "@rollup/plugin-typescript": "11.1.6",
      "@testing-library/react": "14.0.0",
      "@types/chrome": "0.0.251",
      "@types/lodash-es": "^4.17.12",
      "@types/node": "20.8.10",
      "@types/react": "18.2.37",
      "@types/react-dom": "18.2.18",
      "@types/ws": "8.5.8",
      "@typescript-eslint/eslint-plugin": "6.10.0",
      "@typescript-eslint/parser": "6.18.1",
      "@vitejs/plugin-react": "4.2.0",
      "chokidar": "3.5.3",
      "cross-env": "7.0.3",
      "eslint": "8.56.0",
      "eslint-config-airbnb-typescript": "17.1.0",
      "eslint-config-prettier": "9.0.0",
      "eslint-plugin-import": "2.29.1",
      "eslint-plugin-import-newlines": "^1.4.0",
      "eslint-plugin-jsx-a11y": "6.8.0",
      "eslint-plugin-prettier": "5.1.3",
      "eslint-plugin-react": "7.33.2",
      "eslint-plugin-react-hooks": "4.6.0",
      "eslint-plugin-simple-import-sort": "^12.0.0",
      "eslint-plugin-unused-imports": "^3.1.0",
      "fs-extra": "11.1.1",
      "husky": "8.0.3",
      "jsdom": "^22.1.0",
      "lint-staged": "15.2.0",
      "magic-string": "^0.30.6",
      "npm-run-all": "4.1.5",
      "prettier": "3.2.5",
      "prettier-eslint": "^16.3.0",
      "prettier-eslint-cli": "^8.0.1",
      "rollup": "4.3.0",
      "sass": "1.69.5",
      "ts-loader": "9.5.0",
      "tslib": "2.6.2",
      "typescript": "5.2.2",
      "vite": "5.0.12",
      "vite-bundle-visualizer": "^1.0.1",
      "vitest": "^1.2.2",
      "ws": "8.14.2"
   },
   "lint-staged": {
      "*.{js,jsx,ts,tsx}": [
         "prettier --write",
         "eslint --fix"
      ]
   },
   "packageManager": "pnpm@8.9.2"
}
